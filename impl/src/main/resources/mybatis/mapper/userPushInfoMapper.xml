<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="userPushInfo">
    <resultMap id="userPushInfo" type="userPushInfo">
        <result column="uid" property="uid" jdbcType="BIGINT"/>
        <result column="pushType" property="pushType" jdbcType="INTEGER"/>
        <result column="pushToken" property="pushToken" jdbcType="VARCHAR"/>
        <result column="country" property="country" jdbcType="VARCHAR"/>
        <result column="language" property="language" jdbcType="VARCHAR"/>
        <result column="lastLoginTime" property="lastLoginTime" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        uid,pushType,pushToken,country,language,lastLoginTime
    </sql>

    <select id="selectByAudienceCountryCode" resultMap="userPushInfo">
        select
        <include refid="Base_Column_List"/>
        from
        user_push_info

    </select>

</mapper>
